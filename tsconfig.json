{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "./declarations.d.ts",
    "./app/**/*",
    "./node_modules/expo/**/*",
    "./node_modules/react-native/**/*"
, "screen", "app/screens"  ]
}